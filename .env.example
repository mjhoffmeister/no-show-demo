# Azure Configuration
# Copy this file to .env and fill in your values
# DO NOT commit .env to source control

# Required: Azure subscription and location
AZURE_SUBSCRIPTION_ID=your-subscription-id-here
AZURE_LOCATION=northcentralus

# Optional: Resource naming prefix (defaults to 'noshow')
AZURE_RESOURCE_PREFIX=noshow

# Optional: Environment name for resource tagging
AZURE_ENV_NAME=dev

# Azure SQL Database (populated after infra deployment)
# SQL_SERVER_NAME=
# SQL_DATABASE_NAME=

# Azure ML Workspace (populated after infra deployment)
# AZURE_ML_WORKSPACE_NAME=
# AZURE_ML_ENDPOINT_NAME=

# Azure AI Foundry (populated after infra deployment)
# AZURE_AI_PROJECT_NAME=
# AZURE_AI_AGENT_NAME=
