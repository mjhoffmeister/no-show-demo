{
  "routes": [
    { "route": "/authentication/*", "rewrite": "/index.html" },
    { "route": "/chat", "rewrite": "/index.html" },
    { "route": "/chat/*", "rewrite": "/index.html" },
    { "route": "/", "rewrite": "/index.html" }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_framework/*", "/css/*", "/*.js", "/*.json", "/*.ico"]
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "mimeTypes": {
    ".dll": "application/octet-stream",
    ".wasm": "application/wasm",
    ".blat": "application/octet-stream",
    ".dat": "application/octet-stream"
  }
}
